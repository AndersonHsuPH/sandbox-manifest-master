<Manifest>
  <ProjectInfo>
    <CodeName>Tigerlake</CodeName>
    <Description>Tigerlake</Description>
    <DevLead>nathaniel.l.desimone@intel.com</DevLead>
    <LeadReviewers>
      <Reviewer>anderson_hsu@phoenix.com</Reviewer>
    </LeadReviewers>
  </ProjectInfo>

  <GeneralConfig>
    <PinPath>Tigerlake_Codebase/pins</PinPath>
    <DefaultCombo combination="master" />
    <CurrentClonedCombo combination="master" />
  </GeneralConfig>

  <RemoteList>
