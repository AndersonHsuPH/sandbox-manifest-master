<Manifest>
  <ProjectInfo>
    <CodeName>Edk2Platforms</CodeName>
    <Description>Edk2Platforms</Description>
    <DevLead>nathaniel.l.desimone@intel.com</DevLead>
    <LeadReviewers>
      <Reviewer>anderson_hsu@phoenix.com</Reviewer>
    </LeadReviewers>
  </ProjectInfo>

  <GeneralConfig>
    <PinPath>Edk2Platforms/pins</PinPath>
    <DefaultCombo combination="master" />
    <CurrentClonedCombo combination="master" />
  </GeneralConfig>

  <RemoteList>
    <!-- The 'name' attribute for each Remote tag must be unique. -->
        <Remote name="PtlmodEdk2PlatformsPlatIntel">https://github.com/AndersonHsuPH/PtlmodEdk2PlatformsPlatIntel.git</Remote>
        <Remote name="PtlmodEdk2PlatformsSiIntel">https://github.com/AndersonHsuPH/PtlmodEdk2PlatformsSiIntel.git</Remote>
  </RemoteList>

  <ClientGitHookList>
  </ClientGitHookList>

  <CommitTemplates>
  </CommitTemplates>

  <SubmoduleAlternateRemotes>
  </SubmoduleAlternateRemotes>

  <CombinationList>
    <Combination name="master" description="the master branch from edk2 Repo, master branch from edk2-platforms repo, master branch from edk2-non-osi repo, master branch from FSP repo">
      <Source localRoot="Edk2Platforms\Platform\Intel" remote="PtlmodEdk2PlatformsPlatIntel" branch="master" sparseCheckout="true" enableSubmodule="true" />
      <Source localRoot="Edk2Platforms\Silicon\Intel" remote="PtlmodEdk2PlatformsSiIntel" branch="master" sparseCheckout="true" enableSubmodule="true" />
    </Combination>
  </CombinationList>

  <DscList>
    <!-- List of the top-level Dsc's that are used to build this Platform.
         These files will be parsed by EdkRepo to determine pkg dependencies
     -->
  </DscList>
</Manifest>
